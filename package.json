  "repository": {
    "type": "git",
    "url": "https://github.com/kaltura/KMCng.git"
  },
  "description": "",
  "keywords": [
    "kaltura"
  ],
  "author": "https://github.com/kaltura/KMCng/graphs/contributors",
  "bugs": {
    "url": "https://github.com/kaltura/KMCng/issues"
  },
  "homepage": "https://github.com/kaltura/KMCng#readme",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "ci": "npm run ci:jit",
    "ci:jit": "npm run lint && npm run test && npm run build:prod",
    "ci:nobuild": "npm run lint && npm test",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "lint:rules": "npm run lint | grep -e '^src/' | cut -d':' -f2- | sort -u",
    "preclean:install": "npm run clean",
    "setup": "bash ./config/kaltura-ng2/setup.sh",
    "setup:ci": "bash ./config/kaltura-ng2/setup.sh --use fs",
    "setup:lite": "bash ./config/kaltura-ng2/setup.sh -k",
    "rimraf": "rimraf",
    "release": "standard-version",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "start": "npm run server:dev",
    "test": "npm run lint && karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "wml": "wml",
    "wml:watch": "wml start",
    "wml:sync": "wml once"
  },
  "dependencies": {
    "@angular/common": "~2.4.3",
    "@angular/compiler": "~2.4.3",
