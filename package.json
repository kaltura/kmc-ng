  "scripts": {
    "kws": "kws",
    "ng": "ng",
    "start": "ng serve --aot --preserve-symlinks",
    "build": "ng build --aot --preserve-symlinks",
    "prebuild:prod": "node ./scripts/pre-build-prod.js",
    "build:prod": "ng build --prod --preserve-symlinks",
    "build:prod:analyze": "ng build --prod --preserve-symlinks --stats-json && webpack-bundle-analyzer dist/stats.json",
    "postbuild:prod": "node ./scripts/post-build-prod.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "kaltura-libs:upgrade": "npm i @kaltura-ng/mc-{theme,shared}@latest @kaltura-ng/kaltura-{common,logger,primeng-ui,ui}@latest",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json --disableGraph --disablePrivateOrInternalSupport --toggleMenuItems",
    "release:prepare": "kws release --publish=false",
    "release:publish": "kws release --prepare=false",
    "standalone": "node ./scripts/checkout-standalone.js",
    "standalone:update": "npm run kws -- bookmark standalone --update && git commit -am 'chore: update standalone commit' && git push"
  },
